@{
    var verson = DateTime.Now.ToString("yyyyMMddhhmmss");
    string euiLang = ViewBag.Language;
    string lang = euiLang;
    if (euiLang == null)
    {
        euiLang = "zh_CN";
        lang = "zh-CN";
    }
    euiLang = euiLang.Replace("-", "_");
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="description" content="fortrun.com">
    <meta name="author" content="chenrihong">
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jquery-easyui-1.3.2/themes/default/easyui.css?v=" + Html.OutputText(verson))" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jquery-easyui-1.3.2/themes/icon.css?v=" + Html.OutputText(verson))" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/jquery-easyui-1.3.2/demo/demo.css?v=" + Html.OutputText(verson))" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="../assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="../assets/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="../assets/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="../assets/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="../assets/ico/favicon.png">
    <script type="text/javascript" src="@Url.Content("~/Content/jquery-easyui-1.3.2/jquery-1.8.0.min.js?v=" + Html.OutputText(verson))"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/jquery-easyui-1.3.2/jquery.easyui.min.js?v=" + Html.OutputText(verson))"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/jquery-easyui-1.3.2/locale/easyui-lang-" + Html.OutputText(euiLang) + ".js?v=" + Html.OutputText(verson))"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Site.css?v=" + verson)" />
    <script type="text/javascript" src="@Url.Content("~/Scripts/fortrun.eui.quick.dev.js?v=" + Html.OutputText(verson))"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/locale/fortrun-lang." + Html.OutputText(lang) + ".js?v=" + Html.OutputText(verson))"></script>
    <script type="text/javascript">
        window.g_guid = '@ViewBag.Guid';
        window.g_userid = '@ViewBag.UserId';
        window.g_username = '@ViewBag.UserName';
    </script>
    @RenderSection("Header", false)
</head>
<body>
    @RenderBody()
</body>
</html>
